<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.adarsh.entity.Aircraft" table="Aircraft">
<id name="aid" column="A_ID">
<generator class="increment"/>
</id>
<property name="atype" column="A_TYPE" type="string" length="15"/>
<property name="noEngines" column="No_Of_Engines" type="int"/>
<property name="manuCompany" column="Manuf_Name" type="string" length="15"/>
</class> 
</hibernate-mapping>